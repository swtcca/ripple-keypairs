{
  "name": "swtc-keypairs",
  "version": "0.11.2",
  "description": "swtc keypairs",
  "files": [
    "distrib/npm/*",
    "src/*",
    "bin/*",
    "build/*",
    "test/*"
  ],
  "main": "distrib/npm/",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "bn.js": "^4.0.0",
    "brorand": "^1.0.5",
    "elliptic": "^6.4.0",
    "hash.js": "^1.0.3",
    "swtc-address-codec": "^2.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-transform-classes": "^7.3.4",
    "eslint": "^5.14.1",
    "mocha": "^6.0.2"
  },
  "scripts": {
    "compile": "babel -d distrib/npm/ src/",
    "compile-with-source-maps": "babel --optional runtime -s -t -d distrib/npm/ src/",
    "prepublish": "npm test && npm run compile",
    "test": "mocha test/test*js",
    "lint": "if ! [ -f eslintrc ]; then curl -o eslintrc 'https://raw.githubusercontent.com/ripple/javascript-style-guide/es6/eslintrc'; echo 'parser: babel-eslint' >> eslintrc; fi; eslint -c eslintrc src/*.js test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/swtcca/ripple-keypairs.git"
  },
  "bugs": {
    "url": "https://github.com/swtcca/ripple-keypairs/issues"
  },
  "homepage": "https://github.com/swtcca/ripple-keypairs#readme",
  "license": "ISC",
  "readmeFilename": "README.md",
  "babel": {
    "presets": [],
    "plugins": [
      "@babel/plugin-transform-classes"
    ]
  }
}
